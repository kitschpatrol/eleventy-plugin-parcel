{
  "name": "@kitschpatrol/eleventy-plugin-parcel",
  "version": "1.0.10",
  "description": "A plugin integrating the Parcel build tool and dev server with the Eleventy static site generator.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitschpatrol/eleventy-plugin-parcel.git"
  },
  "homepage": "https://github.com/kitschpatrol/eleventy-plugin-parcel",
  "bugs": "https://github.com/kitschpatrol/eleventy-plugin-parcel/issues",
  "author": {
    "name": "Eric Mika",
    "email": "eric@ericmika.com",
    "url": "https://ericmika.com/"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "packageManager": "pnpm@10.20.0",
  "main": ".eleventy.js",
  "keywords": [
    "eleventy",
    "parcel",
    "eleventy-plugin",
    "npm-package"
  ],
  "scripts": {
    "clean": "git rm -f pnpm-lock.yaml ; git clean -fdX",
    "lint": "eslint . && prettier --check .",
    "fix": "eslint . --fix && prettier --write .",
    "release": "bumpp --commit 'Release: %s' && NPM_AUTH_TOKEN=$(op read 'op://Personal/npm/token') && pnpm publish"
  },
  "peerDependencies": {
    "@mischnic/parcel-resolver-root": "^0.4.2",
    "@parcel/config-default": "^2.7.0",
    "@parcel/core": "^2.7.0"
  },
  "dependencies": {
    "http-proxy-middleware": "^2.0.9",
    "lodash.merge": "^4.6.2",
    "lodash.some": "^4.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "11ty": {
    "compatibility": "^2.0.0"
  },
  "devDependencies": {
    "bumpp": "^10.3.1",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2"
  }
}
